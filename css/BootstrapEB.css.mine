/* Reset CSS */
/* Reset CSS */
a,
abbr,
address,
article,
aside,
audio,
b,
blockquote,
body,
canvas,
caption,
cite,
code,
dd,
del,
details,
dfn,
dialog,
div,
dl,
dt,
em,
fieldset,
figcaption,
figure,
footer,
form,
h1,
h2,
h3,
h4,
h5,
h6,
header,
hgroup,
html,
i,
iframe,
img,
ins,
kbd,
label,
legend,
li,
mark,
menu,
nav,
object,
ol,
p,
pre,
q,
samp,
section,
small,
span,
strong,
sub,
summary,
sup,
table,
tbody,
td,
tfoot,
th,
thead,
time,
tr,
ul,
var,
video {
  margin: 0;
  padding: 0;
}
/* Worklight container div */
html {
  height: 100%;
}
#content {
  margin: 0 auto;
  height: 100%;
  position: absolute!important;
  width: 100%;
}
@font-face {
  font-family: NatWestText;
  src: url('NAWTN___.TTF');
}
@font-face {
  font-family: NatWestText;
  src: url('NAWTB___.TTF');
  font-weight: bold;
}
/*Default config*/
.visible-tv {
  display: none !important;
}
/*TV config*/
@media (min-width: 1824px) {
  .hidden-desktop {
    display: inherit !important;
  }
  .visible-desktop {
    display: none !important;
  }
  .visible-tv {
    display: inherit !important;
  }
  .hidden-tv {
    display: none !important;
  }
  #rightPanel {
    width: 19%;
    float: right;
    height: 100%;
    z-index: 9999;
    /* quick hack for demo*/
  
    height: 100%;
    min-height: 320px;
    background: #21204f;
    /* Old browsers */
  
    background: -moz-linear-gradient(left, #21204f 0%, #2c2b6b 51%, #21204f 100%);
    /* FF3.6+ */
  
    background: -webkit-gradient(linear, left top, right top, color-stop(0%, #21204f), color-stop(51%, #2c2b6b), color-stop(100%, #21204f));
    /* Chrome,Safari4+ */
  
    background: -webkit-linear-gradient(left, #21204f 0%, #2c2b6b 51%, #21204f 100%);
    /* Chrome10+,Safari5.1+ */
  
    background: -o-linear-gradient(left, #21204f 0%, #2c2b6b 51%, #21204f 100%);
    /* Opera 11.10+ */
  
    background: -ms-linear-gradient(left, #21204f 0%, #2c2b6b 51%, #21204f 100%);
    /* IE10+ */
  
    background: linear-gradient(to right, #21204f 0%, #2c2b6b 51%, #21204f 100%);
    /* W3C */
  
    filter: progid:dximagetransform.microsoft.gradient(startColorstr='#21204f', endColorstr='#21204f', GradientType=1);
    /* IE6-9 */
  
  }
  #rightPanel h3 {
    color: #fff;
    font-size: 1.6em;
    margin: 10px 0px 10px 10px;
    font-weight: normal;
  }
  #largeNatwestLogo {
    width: 100%;
    height: 50px;
    float: left;
    text-align: center;
    clear: both;
  }
  #largeNatwestLogo img {
    margin-top: 10px;
  }
  #tvSeparator {
    position: fixed;
    float: left;
    width: 80%;
    height: 100%;
    overflow: hidden;
  }
  .content-wrapper {
    float: left;
    width: 100%;
  }
  .navbar-fixed-top,
  .navbar-fixed-bottom {
    position: relative !important;
    width: 100%;
  }
  .tablet .tabs {
    position: absolute !important;
  }
  #signIn {
    float: left;
    width: 100%;
  }
  .si-form-wrapper {
    height: 340px;
  }
  .vCall {
    background: url("../images/videoCall.png") no-repeat 0 0;
    height: 40px;
    margin: 0px;
    width: 40px;
  }
  .call {
    background: url("../images/phoneIcon.png") no-repeat 0 0;
    height: 48px;
    margin: 0px;
    width: 48px;
  }
  .facebook {
    background: url("../images/facebookIcon.png") no-repeat 0 0;
    height: 48px;
    margin: 0px;
    width: 48px;
    float: right;
  }
  .twitter {
    background: url("../images/twitterIcon.png") no-repeat 0 0;
    height: 48px;
    margin: 0px;
    width: 48px;
    float: right;
  }
  .tvHalf {
    width: 48%;
    height: 90px;
    float: left;
    color: #fff;
    text-align: center;
    background: url("../images/butBg.png") repeat-x top;
    border: 1px solid #000;
    margin-left: 4px;
  }
  .tvHalf div {
    float: left;
    margin: 32px 0px 0px 15px;
  }
  .tvHalf p {
    float: left;
    margin: 40px 0px 0px 10px;
    font-size: 1.4em;
  }
  .tvFull {
    width: 100%;
    height: 90px;
    float: left;
    color: #fff;
    border-top: solid 1px #000;
    margin-top: 10px;
  }
  .tvFull p {
    float: left;
    margin: 25px 0px 0px 20px;
    font-size: 1.5em;
  }
  .tvFull div {
    float: right;
    margin: 15px 20px 0px 0px;
  }
  #tvSupportBox {
    position: fixed;
    bottom: 0;
  }
  .container.tvContainer {
    width: auto;
  }
  .tvContainer .nav {
    margin-right: 20px;
  }
  .tvContainer .sideMenu-search {
    width: 300px;
  }
  screen {
    margin-top: 0px !important;
  }
}
/* LANDING : START */
.container {
  margin: 10px;
  width: auto;
}
.primary-content,
.secondary-content.margin-bottom {
  margin-bottom: 10px;
}
.primary-content,
.secondary-content {
  background: #ffffff;
  /* Old browsers */

  background: -moz-radial-gradient(center, ellipse cover, #ffffff 0%, #c4c4c4 100%);
  /* FF3.6+ */

  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, #ffffff), color-stop(100%, #c4c4c4));
  /* Chrome,Safari4+ */

  background: -webkit-radial-gradient(center, ellipse cover, #ffffff 0%, #c4c4c4 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-radial-gradient(center, ellipse cover, #ffffff 0%, #c4c4c4 100%);
  /* Opera 12+ */

  background: -ms-radial-gradient(center, ellipse cover, #ffffff 0%, #c4c4c4 100%);
  /* IE10+ */

  background: radial-gradient(ellipse at center, #ffffff 0%, #c4c4c4 100%);
  /* W3C */

  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ffffff', endColorstr='#c4c4c4', GradientType=1);
  /* IE6-9 fallback on horizontal gradient */
}
/* LANDING : END */
