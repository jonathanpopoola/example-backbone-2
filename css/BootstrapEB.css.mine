/*
Drop down (popover)
*/
.popover-overlay {
  background-color: transparent !important;
  position: fixed;
  top: 0px;
  width: 100%;
  height: 100%;
  z-index: 500;
}
.popover {
  background-color: #2a254b !important;
  background-repeat: repeat-x;
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ceced3', endColorstr='#ececf1');
  background-image: -khtml-gradient(linear, left top, left bottom, from(#2a254b), to(#2a254b));
  background-image: -moz-linear-gradient(top, #2a254b, #2a254b);
  background-image: -ms-linear-gradient(top, #2a254b, #2a254b);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #2a254b), color-stop(100%, #2a254b));
  background-image: -webkit-linear-gradient(top, #2a254b, #2a254b);
  background-image: -o-linear-gradient(top, #2a254b, #2a254b);
  background-image: linear-gradient(#2a254b, #2a254b);
  top: 40px !important;
  right: 0!important;
  padding: 8px;
  z-index: 10000;
}
.popover-inner,
.popover-title {
  background-color: #161137 !important;
  background-repeat: repeat-x;
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ceced3', endColorstr='#ececf1');
  background-image: -khtml-gradient(linear, left top, left bottom, from(#161137), to(#161137));
  background-image: -moz-linear-gradient(top, #161137, #161137);
  background-image: -ms-linear-gradient(top, #161137, #161137);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #161137), color-stop(100%, #161137));
  background-image: -webkit-linear-gradient(top, #161137, #161137);
  background-image: -o-linear-gradient(top, #161137, #161137);
  background-image: linear-gradient(#161137, #161137);
}
.popover-inner {
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  -ms-border-radius: 6px;
  -o-border-radius: 6px;
  border-radius: 6px;
}
.popover-title {
  background-color: #161137 !important;
  background-repeat: repeat-x;
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ceced3', endColorstr='#ececf1');
  background-image: -khtml-gradient(linear, left top, left bottom, from(#161137), to(#161137));
  background-image: -moz-linear-gradient(top, #161137, #161137);
  background-image: -ms-linear-gradient(top, #161137, #161137);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #161137), color-stop(100%, #161137));
  background-image: -webkit-linear-gradient(top, #161137, #161137);
  background-image: -o-linear-gradient(top, #161137, #161137);
  background-image: linear-gradient(#161137, #161137);
  border-bottom: none;
  padding: 0;
}
.popover-content ul {
  display: block !important;
}
.popover-content li {
  font-family: NatWestText !important;
  font-weight: normal !important;
  font-size: 1em !important;
  color: #d8d8dd !important;
  float: none !important;
  padding: 10px 0;
}
.popover-content li a {
  font-family: NatWestText !important;
  font-weight: normal !important;
  font-size: 1em !important;
  color: #d8d8dd !important;
}
.arrow {
  left: 93% !important;
  border-bottom-color: #2a254b !important;
}
/*Bootstrap Accordian/Collapse*/
.accordian {
  margin-bottom: 0;
}
.accordion-group {
  margin-bottom: 0;
  border: none;
  border-top: 1px solid #161137;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}
.accordion-heading .accordion-toggle {
  padding: 20px 20px 20px 0;
}
.inner-toggle {
  font-family: NatWestText !important;
  font-weight: normal !important;
  font-size: 1.1em !important;
  color: #ffffff !important;
}
.main-toggle {
  font-family: NatWestText !important;
  font-weight: normal !important;
  font-size: 1.1em !important;
  color: #9897a9 !important;
}
#collapsePrivateBanking {
  color: #fff;
  background: #3f3e73;
  background: -moz-radial-gradient(center, ellipse cover, #3f3e73 1%, #2a294a 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(1%, #3f3e73), color-stop(100%, #2a294a));
  background: -webkit-radial-gradient(center, ellipse cover, #3f3e73 1%, #2a294a 100%);
  background: -o-radial-gradient(center, ellipse cover, #3f3e73 1%, #2a294a 100%);
  background: -ms-radial-gradient(center, ellipse cover, #3f3e73 1%, #2a294a 100%);
  background: radial-gradient(ellipse at center, #3f3e73 1%, #2a294a 100%);
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='@radial-gradient-start', endColorstr='@radial-gradient-stop', GradientType=1);
}
.accordion-heading {
  position: relative;
}
.accordion-heading a {
  display: inline-block;
}
.accordion-heading .expand-icon {
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -10px;
}
.accordion-inner {
  border-top: 1px solid #d8d8dd;
  padding: 0;
}
#collapseCurrentAccounts {
  background: #c3c3d0;
  background: -moz-linear-gradient(left, #c3c3d0 73%, #9797a2 100%);
  background: -webkit-gradient(linear, left top, right top, color-stop(73%, #c3c3d0), color-stop(100%, #9797a2));
  background: -webkit-linear-gradient(left, #c3c3d0 73%, #9797a2 100%);
  background: -o-linear-gradient(left, #c3c3d0 73%, #9797a2 100%);
  background: -ms-linear-gradient(left, #c3c3d0 73%, #9797a2 100%);
  background: linear-gradient(to right, #c3c3d0 73%, #9797a2 100%);
  filter: progid:dximagetransform.microsoft.gradient(startColorstr='@subinner-start-color', endColorstr='@subinner-stop-color', GradientType=1);
}
.accordion-inner a {
  font-family: NatWestText !important;
  font-weight: bold !important;
  font-size: 1.1em !important;
  color: #0e1546 !important;
  padding: 10px 40px;
  display: block;
}
@media (max-width: 979px) {
  .navbar-bottom .container {
    padding-top: 25px;
  }
  .navbar-fixed-top {
    position: fixed;
    margin-left: 0;
    margin-right: 0;
  }
  .navbar-fixed-top {
    margin-bottom: 0;
  }
  .navbar-fixed-top .navbar-inner,
  .navbar-bottom .navbar-inner {
    padding: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
  }
  /*Remove white edges in mobile browser*/
  .navbar-bottom {
    bottom: -1px;
    margin: 0;
  }
}
/* CMS Styles*/
.cmsContainer {
  -moz-box-shadow: 0px 0px 5px 2px rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: 0px 0px 5px 2px rgba(0, 0, 0, 0.3);
  box-shadow: 0px 0px 5px 2px rgba(0, 0, 0, 0.3);
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  border: 3px solid #fff;
  background-color: #e7e7e7;
  font-family: NatWestText;
}
.cmsContainer fieldset {
  margin-left: 10px;
}
.cmsContainer legend {
  color: #691c6e;
  font-size: 1.625em;
  margin: 20px 0px 10px 0px;
}
.cmsContainer div > label {
  float: left;
  width: 35%;
  margin-right: 5px;
  font-size: 1em;
  margin-right: 20px;
}
.cmsContainer div > input,
textarea,
select {
  float: left;
}
.cms-tabs li {
  text-align: center;
  margin-bottom: -3px;
}
.cms-tabs a {
  background: #e7e7e7;
  color: #979797;
  font-weight: bold;
  font-size: 1.625em;
  width: 130px;
}
.cms-tabs .active a,
.cms-tabs .active a:Hover {
  background: #e7e7e7;
  color: #691c6e;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  border-bottom-right-radius: 0px;
  border-bottom-left-radius: 0px;
  -moz-border-radius-bottomright: 0px;
  -moz-border-radius-bottomleft: 0px;
  -webkit-border-bottom-right-radius: 0px;
  -webkit-border-bottom-left-radius: 0px;
  border: 3px solid #fff;
  border-bottom: 4px solid #e7e7e7;
  margin-right: 10px;
  -moz-box-shadow: 0px 0px 5px 0px #e7e7e7;
  -webkit-box-shadow: 0px 0px 5px 0px #e7e7e7;
  box-shadow: 0px 0px 5px 0px #e7e7e7;
}
#cmsTabsHolder {
  width: 100%;
  float: left;
}
.cms-tabs {
  margin-bottom: 0px;
  margin-left: 10px;
  border: none;
  font-family: NatWestText;
  position: relative;
}
.cmsContainer button {
  float: right;
  font-weight: bold;
  background: #e4e4e4;
  /* Old browsers */

  background: -moz-linear-gradient(top, #e4e4e4 0%, #c2c2c2 100%);
  /* FF3.6+ */

  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #e4e4e4), color-stop(100%, #c2c2c2));
  /* Chrome,Safari4+ */

  background: -webkit-linear-gradient(top, #e4e4e4 0%, #c2c2c2 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-linear-gradient(top, #e4e4e4 0%, #c2c2c2 100%);
  /* Opera 11.10+ */

  background: -ms-linear-gradient(top, #e4e4e4 0%, #c2c2c2 100%);
  /* IE10+ */

  background: linear-gradient(to bottom, #e4e4e4 0%, #c2c2c2 100%);
  /* W3C */

  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#e4e4e4', endColorstr='#c2c2c2', GradientType=0);
  /* IE6-9 */

  border: solid 1px #fff;
  -moz-box-shadow: 0px 0px 3px 0px #000;
  -webkit-box-shadow: 0px 0px 3px 0px #000;
  box-shadow: 0px 0px 3px 0px #000;
  padding: 5px;
  color: #691c6e;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  border-radius: 2px;
  text-shadow: 1px 1px 2px #ffffff;
  filter: dropshadow(color=#ffffff, offx=1, offy=1);
  margin: 10px;
}
.cmsContainer form div {
  float: left;
  width: 100%;
}
.helper {
  display: block;
  width: 24px;
  height: 24px;
  background: url("../images/tooltipIcon.png") no-repeat;
  text-indent: -9999px;
  outline: none;
  float: left;
  margin: 2px 10px 0px 10px;
}
#richText {
  width: 98%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 20px;
  margin-bottom: 10px;
  background: #dddddd;
  /* Old browsers */

  background: -moz-linear-gradient(top, #dddddd 0%, #c3c3c3 100%);
  /* FF3.6+ */

  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #dddddd), color-stop(100%, #c3c3c3));
  /* Chrome,Safari4+ */

  background: -webkit-linear-gradient(top, #dddddd 0%, #c3c3c3 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-linear-gradient(top, #dddddd 0%, #c3c3c3 100%);
  /* Opera 11.10+ */

  background: -ms-linear-gradient(top, #dddddd 0%, #c3c3c3 100%);
  /* IE10+ */

  background: linear-gradient(to bottom, #dddddd 0%, #c3c3c3 100%);
  /* W3C */

  filter: progid:dximagetransform.microsoft.gradient(startColorstr='#dddddd', endColorstr='#c3c3c3', GradientType=0);
  /* IE6-9 */

  -webkit-box-shadow: inset 1px 1px 2px 1px #c4c4c4;
  box-shadow: inset 1px 1px 2px 1px #c4c4c4;
  min-height: 400px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  border: 1px solid #c4c4c4;
}
#richTextControls {
  width: 100%;
  min-height: 30px;
  float: left;
}
#richTextControls ul {
  padding: 0px;
  margin: 5px 0px 0px 0px;
  list-style: none;
}
#richTextControls ul li {
  display: inline;
}
#richText textarea {
  width: 98%;
  background: #e7e7e7;
  -webkit-box-shadow: inset 1px 1px 3px 1px #c4c4c4;
  box-shadow: inset 1px 1px 3px 1px #c4c4c4;
  height: 260px;
  padding: 2px;
  margin: 10px 0px 10px 5px;
}
.cmsSmall {
  width: 48%;
  min-height: 300px;
  float: left;
  margin: 10px 10px 10px 0px;
}
.cmsLarge {
  width: 98%;
  min-height: 300px;
  float: left;
  margin: 0px 0px 10px 0px;
}
#cmsHolder {
  width: 960px;
  height: 100%;
  margin-left: auto;
  margin-right: auto;
}
