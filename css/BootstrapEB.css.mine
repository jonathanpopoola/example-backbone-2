/* RIGHT POPOVER MENU : START */
.popover {
  border-radius: 15px!important;
  -webkit-border-radius: 15px!important;
  -moz-border-radius: 15px!important;
}
.popover-content li {
  padding: 7px!important;
  border-bottom: 1px solid #000000;
  border-top: 1px solid #363361;
}
.popover-content li:first-child {
  border-top: none !important;
}
.popover-content li:last-child {
  border-bottom: none !important;
}
.popover-content {
  padding: 0!important;
}
.popover.bottom .arrow {
  margin-left: -15px !important;
}
.popover-inner {
  border-bottom: 1px solid #4A4A6B;
  border-left: 1px solid #181839;
  border-right: 1px solid #181839;
  border-top: 1px solid #1D1D46;
  border-radius: 10px!important;
  -webkit-border-radius: 10px!important;
  -moz-border-radius: 10px!important;
}
