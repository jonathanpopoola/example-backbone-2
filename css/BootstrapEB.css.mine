#personal-banking-header {
  padding: 20px;
  background: #c9c7d3;
  /* Old browsers */

  /*background-image: url("../images/stripe-back.png"), -moz-linear-gradient(top,  #c9c7d3 0%, #ffffff 35%);
	background-image: url("../images/stripe-back.png"), -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c9c7d3), color-stop(35%,#ffffff));
	background-image: url("../images/stripe-back.png"), -webkit-linear-gradient(top,  #c9c7d3 0%,#ffffff 35%);
	background-image: url("../images/stripe-back.png"), -o-linear-gradient(top,  #c9c7d3 0%,#ffffff 35%); 
	background-image: url("../images/stripe-back.png"), -ms-linear-gradient(top,  #c9c7d3 0%,#ffffff 35%); 
	background-image: url("../images/stripe-back.png"), linear-gradient(to bottom,  #c9c7d3 0%,#ffffff 35%);
	*/

  background-image: -moz-linear-gradient(top, #c9c7d3 0%, #ffffff 35%);
