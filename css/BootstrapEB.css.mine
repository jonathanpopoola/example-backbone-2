  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(22%, rgba(95, 25, 104, 0.92)), color-stop(40%, rgba(95, 25, 104, 0.94)), color-stop(100%, #23143a));
  /* Chrome,Safari4+ */

  background: -webkit-radial-gradient(center, ellipse cover, rgba(95, 25, 104, 0.92) 22%, rgba(95, 25, 104, 0.94) 40%, #23143a 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-radial-gradient(center, ellipse cover, rgba(95, 25, 104, 0.92) 22%, rgba(95, 25, 104, 0.94) 40%, #23143a 100%);
  /* Opera 12+ */

  background: -ms-radial-gradient(center, ellipse cover, rgba(95, 25, 104, 0.92) 22%, rgba(95, 25, 104, 0.94) 40%, #23143a 100%);
  /* IE10+ */

